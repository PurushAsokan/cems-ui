      <!-- Section 1: Degree Programs List -->
      <div class="section" id="degreeProgramsSection">
        <div class="section-header">
          <div style="display: flex; align-items: center; gap: 12px;">
            <h2 class="section-title">Degree Programs</h2>
            <span class="status status-approved">
              <span class="status-dot"></span>
              12 Programs
            </span>
          </div>
          <div style="display: flex; gap: 8px;">
            <input type="search" class="form-input" placeholder="Search programs..." style="width: 250px;" />
            <button class="btn btn-primary" onclick="openAddDegreeModal()">
              <i data-feather="plus" style="width: 16px; height: 16px;"></i>
              Add Degree Program
            </button>
          </div>
        </div>
        <div class="section-body">
          <div class="table-container">
            <table class="data-table">
              <thead>
                <tr>
                  <th>Program Code</th>
                  <th>Program Name</th>
                  <th>Level</th>
                  <th>Department</th>
                  <th style="text-align: center;">Duration (Years)</th>
                  <th style="text-align: center;">Total Semesters</th>
                  <th>Status</th>
