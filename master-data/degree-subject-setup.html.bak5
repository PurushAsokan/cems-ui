  <script src="../js/cems.js"></script>
  <script>
    // Initialize feather icons
    feather.replace();

    // Tab switching functionality
    function switchTab(tabName) {
      // Hide all sections
      document.getElementById('degreeProgramsSection').style.display = 'none';
      document.getElementById('subjectMasterSection').style.display = 'none';

      // Remove active class from all tabs
      document.getElementById('tab-degreePrograms').classList.remove('active');
      document.getElementById('tab-subjectMaster').classList.remove('active');
      document.getElementById('tab-degreePrograms').style.borderBottomColor = 'transparent';
      document.getElementById('tab-subjectMaster').style.borderBottomColor = 'transparent';
      document.getElementById('tab-degreePrograms').style.color = '#6B7280';
      document.getElementById('tab-subjectMaster').style.color = '#6B7280';

      // Show selected section and activate tab
      if (tabName === 'degreePrograms') {
        document.getElementById('degreeProgramsSection').style.display = 'block';
        document.getElementById('tab-degreePrograms').classList.add('active');
        document.getElementById('tab-degreePrograms').style.borderBottomColor = '#2563EB';
        document.getElementById('tab-degreePrograms').style.color = '#2563EB';
        document.getElementById('tab-degreePrograms').style.fontWeight = '600';
      } else if (tabName === 'subjectMaster') {
        document.getElementById('subjectMasterSection').style.display = 'block';
        document.getElementById('tab-subjectMaster').classList.add('active');
        document.getElementById('tab-subjectMaster').style.borderBottomColor = '#2563EB';
        document.getElementById('tab-subjectMaster').style.color = '#2563EB';
        document.getElementById('tab-subjectMaster').style.fontWeight = '600';
      }

      // Re-initialize feather icons
      setTimeout(() => feather.replace(), 0);
    }

    // Add Degree Modal functions
    function openAddDegreeModal() {
      document.getElementById('addDegreeModal').style.display = 'flex';
      setTimeout(() => feather.replace(), 0);
    }

    function closeAddDegreeModal() {
      document.getElementById('addDegreeModal').style.display = 'none';
    }

    function saveDegreeProgram(event) {
      event.preventDefault();
