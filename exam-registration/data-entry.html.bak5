            <a href="../student-portal/results.html" class="module-nav-item module-pages-item">
              <span class="step-number">2</span>
              <span class="step-label">View Results</span>
            </a>
            <a href="../student-portal/hall-ticket.html" class="module-nav-item module-pages-item">
              <span class="step-number">3</span>
              <span class="step-label">Download Hall Ticket</span>
            </a>
          </div>
        </div>

        <!-- ADMINISTRATION Section -->
        <div class="module-section-header">Administration</div>

        <!-- Access Control Module -->
        <div>
          <a href="../access-auth/login.html" class="module-nav-item module-parent" onclick="event.preventDefault(); toggleModule('accessControl')" title="Access Control">
            <div style="display: flex; align-items: center; gap: 8px; flex: 1;">
              <i data-feather="lock" style="width: 18px; height: 18px;"></i>
              <span class="step-label">Access Control</span>
            </div>
            <i data-feather="chevron-down" id="accessControl-chevron" style="width: 16px; height: 16px; transition: transform 0.2s;"></i>
          </a>
          <div id="accessControl-pages" class="module-pages" style="display: none;">
            <a href="../access-auth/login.html" class="module-nav-item module-pages-item">
              <span class="step-number">1</span>
              <span class="step-label">Login</span>
            </a>
            <a href="../access-auth/biometric.html" class="module-nav-item module-pages-item">
              <span class="step-number">2</span>
              <span class="step-label">Biometric Auth</span>
            </a>
            <a href="../access-auth/audit-log.html" class="module-nav-item module-pages-item">
              <span class="step-number">3</span>
              <span class="step-label">Audit Log</span>
            </a>
          </div>
        </div>
      </nav>

      <button class="sidebar-toggle" title="Toggle Sidebar (Ctrl+B)">
        <i data-feather="chevrons-left"></i>
      </button>
    </aside>

    <!-- Main Container -->
    <main class="main-container main-container-with-sidebar">
    <div class="page-container">

      <!-- Application Details -->
      <div class="section">
        <div class="section-header">
          <h2 class="section-title">Application</h2>
        </div>
        <div class="section-body">
          <div class="field-grid">
            <div class="form-group">
              <label class="form-label required">App Ref Number</label>
              <input type="text" class="form-input" placeholder="APP-2025-001234" />
            </div>
            <div class="form-group">
              <label class="form-label">Previous Reg No</label>
              <input type="text" class="form-input" placeholder="24ENG1001" />
            </div>
          </div>
        </div>
      </div>

      <!-- Student Details -->
      <div class="section">
        <div class="section-header">
          <h2 class="section-title">Student</h2>
          <button class="btn btn-sm btn-ghost">Upload Photo</button>
        </div>
        <div class="section-body">
          <div style="display: grid; grid-template-columns: 1fr auto; gap: 24px;">
            <!-- Form Fields -->
            <div style="display: grid; gap: 16px;">
              <div class="form-group">
                <label class="form-label required">Full Name</label>
                <input type="text" class="form-input" placeholder="Enter full name" />
              </div>

              <div style="display: grid; grid-template-columns: repeat(3, 1fr); gap: 16px;">
                <div class="form-group">
                  <label class="form-label required">Date of Birth</label>
                  <input type="date" class="form-input" />
                </div>
                <div class="form-group">
                  <label class="form-label required">Gender</label>
                  <select class="form-select">
                    <option value="">Select</option>
                    <option>Male</option>
                    <option>Female</option>
                    <option>Other</option>
                  </select>
                </div>
                <div class="form-group">
                  <label class="form-label required">Contact</label>
                  <input type="tel" class="form-input" placeholder="9876543210" />
                </div>
              </div>

              <div class="form-group">
                <label class="form-label">Email</label>
                <input type="email" class="form-input" placeholder="student@example.com" />
              </div>

              <div style="display: grid; grid-template-columns: repeat(2, 1fr); gap: 16px;">
                <div class="form-group">
                  <label class="form-label required">Father's Name</label>
                  <input type="text" class="form-input" placeholder="Enter father's name" />
                </div>
                <div class="form-group">
                  <label class="form-label">Father's Aadhaar Number</label>
                  <input type="text" class="form-input" placeholder="12 digit Aadhaar" maxlength="12" pattern="[0-9]{12}" />
                </div>
              </div>

              <div style="display: grid; grid-template-columns: repeat(2, 1fr); gap: 16px;">
                <div class="form-group">
                  <label class="form-label required">Mother's Name</label>
                  <input type="text" class="form-input" placeholder="Enter mother's name" />
                </div>
                <div class="form-group">
                  <label class="form-label">Mother's Aadhaar Number</label>
                  <input type="text" class="form-input" placeholder="12 digit Aadhaar" maxlength="12" pattern="[0-9]{12}" />
                </div>
              </div>

              <div style="display: grid; grid-template-columns: repeat(4, 1fr); gap: 16px;">
                <div class="form-group">
                  <label class="form-label required">Category</label>
                  <select class="form-select">
                    <option value="">Select</option>
                    <option>General</option>
                    <option>OBC</option>
                    <option>SC</option>
                    <option>ST</option>
                  </select>
                </div>
                <div class="form-group">
                  <label class="form-label">Caste</label>
                  <input type="text" class="form-input" placeholder="Enter caste" />
                </div>
                <div class="form-group">
                  <label class="form-label">Aadhaar Number</label>
                  <input type="text" class="form-input" placeholder="12 digit Aadhaar" maxlength="12" />
                </div>
                <div class="form-group">
                  <label class="form-label">Nationality</label>
                  <input type="text" class="form-input" value="INDIAN" readonly />
                </div>
              </div>

              <div class="form-group">
                <label class="form-label required">Address (Communication)</label>
                <textarea class="form-input" rows="2" placeholder="Enter current communication address"></textarea>
              </div>
            </div>

            <!-- Photo Upload -->
            <div style="width: 140px;">
              <label class="form-label">Photo</label>
              <div style="width: 120px; height: 140px; border: 2px dashed #E5E7EB; border-radius: 4px; display: flex; align-items: center; justify-content: center; background: #F8F9FA; cursor: pointer;">
                <div style="text-align: center; color: #6B7280; font-size: 11px;">
                  <i data-feather="user" style="width: 32px; height: 32px; margin: 0 auto 8px; display: block;"></i>
                  <div>Click to<br/>upload</div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>

      <!-- Academic Details -->
      <div class="section">
        <div class="section-header">
          <h2 class="section-title">Academic</h2>
        </div>
        <div class="section-body">
          <div style="display: grid; grid-template-columns: repeat(4, 1fr); gap: 16px;">
            <div class="form-group">
              <label class="form-label required">Program</label>
              <select class="form-select">
                <option value="">Select</option>
                <option>B.Tech - CSE</option>
                <option>B.Tech - ECE</option>
                <option>B.Tech - MECH</option>
              </select>
            </div>
            <div class="form-group">
              <label class="form-label required">Department</label>
              <select class="form-select">
                <option value="">Select</option>
                <option>Computer Science</option>
                <option>Electronics</option>
                <option>Mechanical</option>
              </select>
            </div>
            <div class="form-group">
              <label class="form-label required">Year</label>
              <select class="form-select">
                <option value="">Select</option>
                <option>1</option>
                <option>2</option>
                <option>3</option>
                <option>4</option>
              </select>
            </div>
            <div class="form-group">
              <label class="form-label">Year of Admission</label>
              <select class="form-select">
                <option value="">Select</option>
                <option>2025</option>
                <option>2024</option>
                <option>2023</option>
                <option>2022</option>
              </select>
              <div class="text-xs" style="color: #6B7280; margin-top: 4px;">Academic year when student joined</div>
            </div>
          </div>
        </div>
      </div>

      <!-- Form Actions -->
      <div class="section">
        <div class="section-body" style="display: flex; justify-content: flex-end; gap: 12px; padding-top: 8px; padding-bottom: 8px;">
          <button class="btn btn-secondary">
            <i data-feather="x" style="width: 16px; height: 16px;"></i>
            Clear Form
          </button>
          <button class="btn btn-primary">
            <i data-feather="check-circle" style="width: 16px; height: 16px;"></i>
            Submit for Verification
          </button>
        </div>
      </div>

    </div>
  </main>

  </div> <!-- End main-with-sidebar -->

  <script src="../js/cems.js"></script>
  <script>
    feather.replace();
  </script>
</body>
</html>
